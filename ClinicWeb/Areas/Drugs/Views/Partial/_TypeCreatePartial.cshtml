@model ClinicWeb.Areas.Drugs.Models.PharmacyTTypeList

<div class="x_panel">
    <div class="x_title">
        <h2>新增劑型</h2>

        <div class="clearfix"></div>
    </div>
    <div class="x_content">
        <br>
        <form id="TypeForm" class="form-label-left input_mask">

            <!--劑型編號-->
            <div class="form-group row">
                <label class="col-form-label col-md-3 col-sm-3 ">劑型編號</label>
                <div class="col-md-9 col-sm-9 ">
                    <input type="text" class="form-control" placeholder="劑型編號" name="fTypeCode">
                </div>
            </div>
            <!--劑型名稱-->
            <div class="form-group row">
                <label class="col-form-label col-md-3 col-sm-3 ">劑型名稱</label>
                <div class="col-md-9 col-sm-9 ">
                    <input type="text" class="form-control" placeholder="劑型名稱" name="fType">
                </div>
            </div>
            <!--按鈕-->
            <div class="form-group row" style="margin:30px">
                <div class="col-md-9 col-sm-9  offset-md-8">

                    <button id="closeButton" type="button" class="btn btn-secondary" data-dismiss="modal"> 關閉 </button>
                    <button class="btn btn-warning" type="reset"> 清除 </button>
                    <button id="saveButton" type="submit" class="btn btn-primary"> 新增 </button>
                </div>
            </div>
        </form>
    </div>
</div>