{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "datatables@1.10.21",
      "destination": "wwwroot/lib/datatables/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@7.0.14",
      "destination": "wwwroot/lib/microsoft/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.js.map",
        "dist/browser/signalr.min.js.map"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "emojione@4.5.0",
      "destination": "wwwroot/lib/emojione/"
    }
  ]
}